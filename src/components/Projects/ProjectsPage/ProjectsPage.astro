---
import ProjectCard from "../ProjectCard/ProjectCard.astro";
import { projectsList } from "./Projects.constants.astro";

export interface ProjectProps {
    title: string;
    imgUrl: string;
    desc: string;
    url: string;
}


---

<div class="bg-transparent w-full h-fit rounded-md text-white flex items-center justify-start gap-5 flex-wrap">
    {projectsList.map((project) => <ProjectCard {...project} />)}
</div>
